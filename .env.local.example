# Blockchain config - REQUIRED for contract deployment
SEPOLIA_URL=https://eth-sepolia.g.alchemy.com/v2/YOUR_ALCHEMY_API_KEY
PRIVATE_KEY=YOUR_WALLET_PRIVATE_KEY
ETHERSCAN_API_KEY=YOUR_ETHERSCAN_API_KEY

# Contract addresses (will be updated by deploy script)
NEXT_PUBLIC_VOTING_CONTRACT_ADDRESS=YOUR_VOTING_CONTRACT_ADDRESS
NEXT_PUBLIC_GOVERNANCE_TOKEN_ADDRESS=YOUR_GOVERNANCE_TOKEN_ADDRESS

# Firebase config - REQUIRED for frontend
NEXT_PUBLIC_FIREBASE_API_KEY=YOUR_FIREBASE_API_KEY
NEXT_PUBLIC_FIREBASE_AUTH_DOMAIN=YOUR_PROJECT_ID.firebaseapp.com
NEXT_PUBLIC_FIREBASE_PROJECT_ID=YOUR_PROJECT_ID
NEXT_PUBLIC_FIREBASE_STORAGE_BUCKET=YOUR_PROJECT_ID.appspot.com
NEXT_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=YOUR_MESSAGING_SENDER_ID
NEXT_PUBLIC_FIREBASE_APP_ID=YOUR_FIREBASE_APP_ID

# Alchemy alternative (used by Hardhat TS config)
ALCHEMY_API_KEY=YOUR_ALCHEMY_API_KEY

# Logging configuration
# Available levels: DEBUG, INFO, WARN, ERROR, NONE
# In production, setting to ERROR is recommended
NEXT_PUBLIC_LOG_LEVEL=DEBUG

# Ethereum Configuration
# Add any other environment variables your app needs
# NEXT_PUBLIC_DEFAULT_NETWORK=sepolia 